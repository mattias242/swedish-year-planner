# Scaleway Configuration
# Copy this file to .env and fill in your values

# Scaleway API credentials
SCW_ACCESS_KEY=your-access-key-here
SCW_SECRET_KEY=your-secret-key-here
SCW_DEFAULT_PROJECT_ID=your-project-id-here

# Deployment configuration
ENVIRONMENT=prod
REGION=fr-par
ZONE=fr-par-1

# Optional: Custom domain (if you have one)
# CUSTOM_DOMAIN=yearplanner.yourdomain.com

# Development
NODE_ENV=production